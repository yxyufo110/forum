/* pages/user/notice/index.wxss */
.cellTitle {
  color: #000000;
  font-weight: 500;
  overflow: hidden;
  text-overflow: ellipsis;
  font-size: 15px;
  white-space: nowrap;
}
.cellLabel {
  overflow: hidden;
  text-overflow: ellipsis;
  color: #999999;
  font-size: 12px;
  white-space: nowrap;
  margin-right: 58px;
}

.sellBox {
  padding: 22px 0;
  margin: 0 22px;
  border-bottom: 1px solid #ebebeb;
  position: relative;
}
.cellTimer {
  font-size: 12px;
  color: #999999;
  white-space: nowrap;
  margin-left: 70px;
}
.box {
  display: flex;
  justify-content: space-between;
}
.box:last-child {
  margin-top: 12px;
}
.view {
  width: 8px;
  height: 8px;
  background: #ff4a4a;
  border-radius: 8px;
  position: absolute;
  left: -12px;
  top: 28px;
}
