.title {
  font-size: 15px;
  color: #000000;
  font-weight: 500;
}
.titleBox {
  padding: 20px 17px 25px 7px;
  display: flex;
  justify-content: space-between;
}
.levelBox {
  font-size: 15px;
  color: #000000;
  padding: 0 17px 0 7px;
  font-weight: 500;
  margin-bottom: 25px;
  margin-top: 5px;
}
.level {
  border-bottom: 1px solid #e9e9e9;
  padding-bottom: 14px;
}
.box {
  display: flex;
  flex-direction: row;
  flex-wrap: wrap;
  justify-content: space-around;
}
.box:after {
  content: ' ';
  width: 40%;
  height: 34px;
}
.item {
  width: 40%;
  height: 34px;
  line-height: 34px;
  text-align: center;
  font-size: 15px;
  font-weight: 500;
  color: #ffffff;
  border-radius: 17px;
  background-color: #a9c5ff;
  margin-bottom: 26px;
}
