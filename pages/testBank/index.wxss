.swiper {
  width: 100%;
  height: 137px;
  border-radius: 5px 5px 0 0;
  overflow: hidden;
  transform: translateY(0);
}
.banner {
  width: 100%;
  height: 100%;
}
.title {
  font-size: 17px;
  margin: 29px 0 6px 4%;
}

.myCell {
  width: 92%;
  margin-left: 4%;
  padding-bottom: 20px;
}
.swiperBox {
  width: 96%;
  margin-left: 2%;
  margin-top: 20px;
  border-radius: 5px;
  overflow: hidden;
  box-shadow: 0px 0px 18px 0px rgba(102, 167, 204, 0.45);
  -moz-box-shadow: 0px 0px 18px 0px rgba(102, 167, 204, 0.45);
  -webkit-box-shadow: 0px 0px 18px 0px rgba(102, 167, 204, 0.45);
}
.cellImg {
  width: 37pt;
  height: 37pt;
}

.cellText {
  font-size: 12.5px;
  color: #000000;
  margin-top: 10px;
}
