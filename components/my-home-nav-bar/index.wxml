<van-nav-bar title="{{title}}" border="{{false}}">
	<view slot="left" class="navIconBox">
		<van-icon
		 name="arrow-left"
		 class="navIcon"
		 bindtap="goBack"
		 custom-style="padding-left:20px;padding-right:15px"
		/>
		<view class="divider">|</view>
		<van-icon
		 name="wap-home"
		 class="navIcon"
		 bindtap="goHome"
		 custom-style="padding-left:12px;padding-right:13px"
		/>
	</view>
</van-nav-bar>

