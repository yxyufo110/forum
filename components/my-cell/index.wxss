.star {
  width: 12px;
  height: 12px;
  margin-right: 6px;
  float: left;
}
.title {
  font-size: 15px;
  color: #000000;
  font-weight: 500;
  margin-bottom: 8px;
}
.box {
  display: flex;
  /* justify-content: space-between; */
  padding: 20px 0 2px 0;
}
.current {
  color: #3875f4;
  font-size: 10px;
}
.all {
  color: #999999;
  font-size: 10px;
}
.speed {
  margin-bottom: 8px;
}
.arrow {
  color: #999999;
  font-size: 15px;
  margin-left: auto;
}
.content {
  display: flex;
  flex-direction: column;
  margin-left: 15px;
}
.coverImg {
  width: 28%;
  height: 70px;
}
